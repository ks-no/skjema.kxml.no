						<xs:documentation>Dokumentert egenkontroll ved operatør.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="1b">
					<xs:annotation>
						<xs:documentation>Dokumentert egenkontroll ved sidemann.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="1c">
					<xs:annotation>
						<xs:documentation>Dokumentert egenkontroll ved overordnet.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value="2">
					<xs:annotation>
						<xs:documentation>Uavhengig kontroll.</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ansvarsrett">
		<xs:annotation>
			<xs:documentation>Søknad om ansvarsrett.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="part" minOccurs="0">
