						<xs:sequence>
							<xs:element name="funksjon" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="kode" type="xs:string" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="referansetilansvarligforetak" minOccurs="0">
								<xs:complexType>
									<xs:attribute name="ref" type="xs:IDREF" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="soknadendringtillatelse">
		<xs:annotation>
			<xs:documentation>Søknad om endring av tillatelse.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="beskrivelse" type="xs:string" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Beskrivelse av endring.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="igangsettingstillatelse">
		<xs:annotation>
			<xs:documentation>Søknad om igangsettingstillatelse.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="gjelderheletiltaket" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Gjelder søknaden om igangsettingstillatelse hele tiltaket?</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="delsoknad" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Delsøknad.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="kommunenssaksnummer" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Kommunens saksnummer.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="delavtiltaket" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Hvilken del av tiltaket gjelder søknaden?</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tillatelsedato" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Dato for gitt tillatelse.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="kommentar" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Kommentar.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="lokalgodkjenning">
		<xs:annotation>
			<xs:documentation>Søknad om lokal godkjenning.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ertidligeregodkjent" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Har det vært søkt om lokal godkjenning i kommunen tidligere?</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="kommunesaksnummer" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Oppgi kommunens saksnummer.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="utdanningsniva" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Utdanningsnivå hos foretakets ledere, antall som har fag/svennebrev.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="grad" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Utdanningsgrad/type skole.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="fag/svennebrev"/>
										<xs:enumeration value="mesterbrev/fagskole"/>
										<xs:enumeration value="ingeniørhøgskole"/>
										<xs:enumeration value="teknisk skole/universitet"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="antall" type="xs:nonNegativeInteger" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Antall i foretaket med denne graden.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="antallpraksisar" type="xs:nonNegativeInteger" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Antall praksisår for ansatte med denne graden.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="beskrivelseavutdannelse" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Beskrivelse av utdannelse ved økt utdanning.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="personerifagligledelse" type="xs:string" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Personer i faglig ledelse.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="system" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Foretaket har systemer i aktivt bruk som bla. omfatter ...</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="systemtype" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Systemtype.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="organisasjonsplan"/>
										<xs:enumeration value="identifikasjon av ..."/>
										<xs:enumeration value="system for ..."/>
										<xs:enumeration value="styring av ..."/>
										<xs:enumeration value="foretaket bruker system som ..."/>
										<xs:enumeration value="planlegging ..."/>
										<xs:enumeration value="regelmessig gjennomgang ..."/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
							<xs:element name="eribruk" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Er i systemet i bruk?</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="referanseprosjekter" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Referanseprosjekter.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="tidligereinnsendt" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Har det tidligere vært innsendt oversikt.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="prosjekt" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Referanseprosjekt.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ansvarsomrader" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Beskrivelse av ansvarsområde.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="adresse" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Referanseprosjektets adresse.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="fagligleder" type="xs:string" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Faglig leder for prosjektet.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="midlertidigbrukstillatelse">
		<xs:annotation>
			<xs:documentation>Anmoding om midlertidig brukstillatelse.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="datoferdigattest" type="xs:date" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Planlagt dato for søknad om ferdigattest.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="gjelderheletiltaket" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Gjelder søknaden om midlertidig brukstillatelse hele tiltaket?</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="delsoknad" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Delsøknad.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="delavtiltaket" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Beskriv hvilken del av tiltaket det søkes om midlertidig brukstillatelse for.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="gjenstaendeinnenfor" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Angi gjenstående arbeider av mindre vesentlig betydning, innenfor den delen av tiltaket det søkes midlertidig brukstillatelse for</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="gjenstaendeutenfor" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Angi resterende deler av tiltaket hvor det her ikke søkes om midlertidig brukstillatelse.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="hartilstrekkeligsikkerhet" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Har tiltaket pr i dag tilstrekkelig sikkerhetsnivå til å tas i bruk?</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="utfortinnen" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Følgende arbeider vil bi utført innen dato.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="typearbeider" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Type arbeider.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="bekreftelseinnen" type="xs:date" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Bekreftelse på at disse arbeidene er utført vil være kommunen i hende innen dato.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="rammebetingelser">
		<xs:annotation>
			<xs:documentation>Tiltakets ytre rammer og bygningsspesifikasjon.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="generellevilkar" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Generelle vilkår.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="oppfyllesvilkar" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Oppfylles vilkårene for 3 ukers saksbehandling, jf. §21-7 annet ledd?</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="berorertidligere1850" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Berører tiltaket bygg eller anlegg fra før 1850?</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sokesomfraviktek" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Vil det søkes om fravik fra TEK10 for eksisterende byggverk (pbl §31-2)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="forhandskonferanseavholdt" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Er forhåndskonferanse avholdt.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="palagtuavhengigkontroll" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Er det pålagt uavhengig kontroll i tiltaket.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="berorerarbeidsplasser" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Berører tiltaket eksisterende eller fremtidige arbeidsplasser?</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="planstatus" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reguleringsplan.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="plantype" type="be:PlantypeType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Plantype.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="navn" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Planens navn.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="formal" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Beskrivelse av reguleringsformål i arealdel av kommuneplan/reguleringsplan/bebyggelsesplan.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="andrerelevantekrav" type="xs:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Andre relevante krav (kommunale vedtekter o.l.)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="beregningsregel" type="be:BeregningsregelType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Beregningsregel grad av utnytting iht. gjeldende plan.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="utnyttingsgrad" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Grad av utnytting iht. gjeldende plan.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:decimal">
										<xs:fractionDigits value="1"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="kravtilbyggegrunn" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Grunnforhold, rasfare og miljøforhold.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="flomutsattomrade" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Plassering i flomutsatt område (TEK §7-2)?</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="skalplasseres" type="be:ThreeStateType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Skal byggverket plasseres i flomutsatt område (TEK §7-2)?</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="f1" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>F1 (liten konsekvens og sannsynlighet lavere enn 1/20 år)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="f2" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>F2 (middels konsekvens og sannsynlighet lavere enn 1/200 år)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="f3" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>F3 (stor konsekvens og sannsynlighet lavere enn 1/1000 år)</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="skredutsattomrade" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Plassering i skredutsatt område (TEK§7-3)?</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
