											<xs:documentation>Fagområde.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ingenvesentligemanglerbrukstillatelse" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Sluttkontrollen har ikke avdekket feil eller mangler som hindrer midlertidig brukstillatelse.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="mindremangler" type="xs:string" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Følgende mangler av mindre vesentlig betydning gjenstår. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="ingenvesentligemanglerferdigattest" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Sluttkontrollen har ikke avdekket feil eller mangler som hindrer ferdigattest.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ansvarsfordeling">
		<xs:annotation>
			<xs:documentation>Kontrollplan.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ansvarligsoker" type="be:AnsvarligSokerType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Ansvarlig søker.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ansvarligprosjektering" type="be:AnsvarligProsjekteringType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Ansvarlig prosjektering.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ansvarligutforelse" type="be:AnsvarligUtforelseType" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Ansvarlig utførelse.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ansvarligkontrollprosjektering" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Ansvarlig kontroll av prosjektering.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="be:AnsvarligKontrollType">
								<xs:sequence>
									<xs:element name="kontrollerkleringrammetillatelse" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Kontrollerklæring vil foreligge ved søknad om rammetillatelse.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="kontrollerkleringigangsettingstillatelse" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Kontrollerklæring vil foreligge ved søknad om ignangsettingstillatelse.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="kontrollerkleringferdigattest" type="xs:boolean" minOccurs="0">
										<xs:annotation>
											<xs:documentation>Kontrollerklæring vil foreligge ved ferdigattest.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="ansvarligkontrollutforelse" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Ansvarlig kontroll av uførelse.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="be:AnsvarligKontrollType"/>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="utforesved">
		<xs:annotation>
			<xs:documentation>Kontroll utføres ved.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="1a">
